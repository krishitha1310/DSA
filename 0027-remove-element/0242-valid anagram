class Solution {
    public boolean isAnagram(String s, String t) {
        int[] h1=new int[27];
        int[] h2=new int[27];
        boolean p=true;
        for(int i=0;i<s.length();i++){
            h1[(int)((s.charAt(i))-96)]++;
        }
         for(int i=0;i<t.length();i++){
            h2[(int)((t.charAt(i))-96)]++;
        }
        for(int i=1;i<27;i++){
            if(h1[i]!=h2[i]){
                p=false;
                break;
            }
        }
    return p;
    }
}
